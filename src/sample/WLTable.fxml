<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<TableView fx:id="bookTableViewWL" fixedCellSize="30.0" minWidth="380.0" fx:controller="sample.WLTableController"
           prefHeight="432.0" prefWidth="759.0" translateY="5.0" VBox.vgrow="NEVER"
           xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <columns>
        <TableColumn fx:id="titleColumnWL" editable="false" minWidth="246.0"
                     prefWidth="360.0" resizable="false" sortable="false" style="-fx-alignment: CENTER-RIGHT;" text="Title" />
        <TableColumn fx:id="authorColumnWL" editable="false" minWidth="246.0" prefWidth="360.0" resizable="false" sortable="false" text="Author" />
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
    <VBox.margin>
        <Insets bottom="42.0" left="10.0" right="10.0" top="20.0" />
    </VBox.margin>
</TableView>
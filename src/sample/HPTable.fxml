<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<TableView fx:id="myBooksTableView" fixedCellSize="30.0" minWidth="380.0"
           xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
           prefHeight="432.0" prefWidth="759.0" translateY="5.0" VBox.vgrow="ALWAYS" fx:controller="sample.HPTableController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <columns>
        <TableColumn fx:id="titleColumn" editable="false" minWidth="240.0" prefWidth="240.0" resizable="false" sortable="false" style="-fx-alignment: CENTER-RIGHT;" text="Title" />
        <TableColumn fx:id="authorColumn" editable="false" minWidth="240.0" prefWidth="240.0" resizable="false" sortable="false" text="Author" />
        <TableColumn fx:id="ratingColumn" editable="false" minWidth="240.0" prefWidth="240.0" resizable="false" sortable="false" text="Rating" />
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
    <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
    </VBox.margin>
</TableView>